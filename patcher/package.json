{
  "name": "cu-patcher-ui",
  "version": "0.1.0",
  "description": "Camelot Unchained Patcher UI",
  "license": "MPL-2.0",
  "main": "src/ts/main.tsx",
  "author": "City State Entertainment <support@citystateentertainment.com>",
  "contributors": [
    "JB (codecorsair) <jb@codecorsair.com>",
    "Mehuge <mehuge@sorcerer.co.uk>",
    "Stacey Schlenker (Sschlenker) <cloudsift@gmail.com>",
    "Brian Bothwell (sysrage) <sysrage@gmail.com>"
  ],
  "private": true,
  "keywords": [
    "Camelot",
    "Unchained",
    "cu",
    "cu-patcher-ui"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/csegames/cu-patcher-ui.git"
  },
  "config": {
    "name": "cu-patcher-ui"
  },
  "scripts": {
    "postinstall": "rimraf typings && typings install",
    "copy:misc": "copyup src/third-party/**/* src/sounds/**/* src/images/**/* src/font/**/* src/**/*.html src/videos/**/* dist",
    "babel": "babel tmp -d tmpp",
    "browserify": "browserify tmpp/ts/main.js -o dist/js/%npm_package_config_name%.js",
    "prebrowserify": "mkdirp dist/js",
    "build:sass": "node-sass src/sass -o dist/css --importer node_modules/camelot-unchained/lib/third-party/sass-importer/sass-npm-importer.js",
    "build": "npm run build:sass && npm run copy:misc -s && tsc && npm run babel && npm run browserify",
    "postbuild": "rimraf tmp && rimraf tmpp",
    "prebuild": "rimraf tmp && rimraf tmpp && rimraf dist",
    "publish": "npm run build",
    "serve": "http-server -p 9000 dist/",
    "debug": "npm run build && npm run copy:patcher && npm run patcher",
    "copy:patcher": "copyup dist/**/* PatchClient/cpui/UI",
    "patcher": "cd Patchclient && start CamelotUnchained.exe canPatchSelf=0 outputUI=0"
  },
  "dependencies": {
    "camelot-unchained": "^0.4.16",
    "cu-character-creation": "^0.1.2",
    "cu-xmpp-chat": "^0.1.4",
    "es6-promise": "^3.0.2",
    "eventemitter3": "^1.1.1",
    "isomorphic-fetch": "^2.2.1",
    "moment": "^2.13.0",
    "node-xmpp-client": "^2.1.0",
    "normalizr": "^2.0.0",
    "react": "^0.14.7",
    "react-addons-css-transition-group": "^0.14.7",
    "react-dom": "^0.14.7",
    "react-redux": "^4.4.5",
    "react-tap-event-plugin": "^0.2.1",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "reflux": "^0.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.5.1",
    "babel-core": "^6.5.2",
    "babel-preset-es2015": "^6.5.0",
    "browserify": "^13.0.0",
    "copyfiles": "^1.0.0",
    "http-server": "^0.9.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^3.4.2",
    "rimraf": "^2.5.2",
    "typescript": "^1.8.10",
    "typings": "^1.3.2"
  }
}
